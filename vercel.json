{
  "version": 2,
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "outputDirectory": "dist",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://topic-explorer-backend-production.up.railway.app/api/:path*"
    }
  ],
  "env": {
    "VITE_SUPABASE_URL": "Enter your Supabase URL here",
    "VITE_SUPABASE_ANON_KEY": "Enter your Supabase Anon Key here",
    "VITE_OPENAI_API_KEY": "Enter your OpenAI API Key here"
  }
} 